<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.kgc.dao.AccountDao">
    <!--根据账号查询账户-->
    <select id="getAccountByNum" resultType="account">
        select * from account where accountNumber=#{accountNumber}
    </select>
    <update id="updatePwdByNum">
        update account set accountPwd=#{accountPwd} where accountNumber=#{accountNumber} ;
    </update>

</mapper>